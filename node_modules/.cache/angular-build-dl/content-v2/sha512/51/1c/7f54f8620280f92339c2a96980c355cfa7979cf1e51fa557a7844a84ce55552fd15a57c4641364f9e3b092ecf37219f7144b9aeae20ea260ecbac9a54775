ome';\n    this.AuHotelLink = 'https://www.auhcc.com/';\n    this.maysaraLogo = './assets/images/Maysara-img.png';\n    this.cWaymanLogo = './assets/images/C.Wayman-logo.png';\n    this.AuHotelLogo = './assets/images/AU-hotel-img.png';\n    this.tableSetting2 = './assets/images/tableSetting2.png';\n    this.tableSetting = './assets/images/tableSetting.png';\n    this.reception = './assets/images/reception-1.png';\n    this.plate = './assets/images/plate.png';\n    this.me = './assets/images/me.png';\n    this.event = './assets/images/event.png';\n    this.navy = './assets/images/navy.png';\n    this.white = './assets/images/white.png';\n    this.black = './assets/images/blk.png';\n  }\n\n  ngOnInit() {\n  }\n\n}\n","export default \".pageBg {\\n  position: absolute;\\n  width: 100%;\\n  height: 100%;\\n  z-index: -2;\\n}\\n\\n.overlay {\\n  position: absolute;\\n  width: 100%;\\n  height: 100%;\\n  background-color: rgba(0, 0, 0, 0.1);\\n}\\n\\n.display-3 {\\n  position: absolute;\\n  top: 8%;\\n  left: 50%;\\n  transform: translate(-50%, -50%);\\n  text-align: center;\\n  color: white;\\n  font-size: 3.8vw;\\n  margin: 0;\\n  padding: 0.5vw 1.6vw 0.8vw 1.6vw;\\n  border: 1px solid white;\\n}\\n\\n.ambassador {\\n  position: absolute;\\n  top: 27%;\\n  left: 50%;\\n  transform: translate(-50%, -50%);\\n  width: 55%;\\n  height: auto;\\n  color: white;\\n  background-color: rgba(0, 0, 0, 0.6);\\n  border-radius: 30px;\\n}\\n\\n.emerge {\\n  position: absolute;\\n  top: 50.5%;\\n  left: 50%;\\n  transform: translate(-50%, -50%);\\n  width: 55%;\\n  height: auto;\\n  color: white;\\n  background-color: rgba(0, 0, 0, 0.6);\\n  border-radius: 30px;\\n}\\n\\n.sorority {\\n  position: absolute;\\n  top: 80%;\\n  left: 50%;\\n  transform: translate(-50%, -50%);\\n  width: 55%;\\n  height: auto;\\n  color: white;\\n  background-color: rgba(0, 0, 0, 0.6);\\n  border-radius: 30px;\\n}\\n\\n.blockquote {\\n  text-align: center;\\n  font-size: 1.5vw;\\n  padding: 0;\\n  margin: 